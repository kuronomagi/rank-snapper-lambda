{
  "name": "ranking-scraper-lambda",
  "version": "1.0.0",
  "description": "スクレイピングとスクリーンショット取得とGoogle Driveアップロード用Lambda関数",
  "main": "dist/index.js",
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "dev": "tsx watch index.ts",
    "start": "node dist/index.js",
    "local": "npm run build && node dist/index.js",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext .ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "deploy": "npm run build && serverless deploy --aws-profile stg-fukuramu-lambda-deployment-user",
    "deploy:prod": "npm run build && serverless deploy --stage prod --aws-profile stg-fukuramu-lambda-deployment-user"
  },
  "keywords": [
    "scraping",
    "screenshot",
    "lambda",
    "aws",
    "google-drive"
  ],
  "dependencies": {
    "@sparticuz/chromium": "^133.0.0",
    "dotenv": "^16.4.7",
    "googleapis": "^148.0.0",
    "puppeteer-core": "^24.6.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "eslint": "^9.17.0",
    "puppeteer": "^24.6.1",
    "serverless": "^4.12.0",
    "serverless-offline": "^14.4.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  }
}
